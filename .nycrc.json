{
    "extension": [".ts"],
    "exclude": [
        "**/*.d.ts",
        "**/node_modules/**",
        "**/test/**",
        "**/coverage/**",
        "**/docs/**",
        "**/compilers/**",
        "**/setup/**"
    ],
    "reporter": ["html", "text-summary"],
    "all": true,
    "check-coverage": true,
    "statements": 90,
    "branches": 85,
    "functions": 90,
    "lines": 95
}
